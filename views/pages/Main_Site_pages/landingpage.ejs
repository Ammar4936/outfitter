  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
  <title>Outfitters Official Online Store</title>
  <style>
    /* Custom CSS for the parallax effect */
  </style>
</head>

<body>

  <!-- Carousel Container -->
  <div class="carousel-container w-full h-screen overflow-y-auto">
    <div id="top">
      <div class="left">
        <div id="menu">
          <div class="line line-big"></div>
          <div class="line line-small"></div>
          <div class="menu-container">
            <div class="menu-top">
              <a class="menu-logo" href="Index.html">OUTFITTERS</a>
            </div>
            <div class="menu-middle">
              <a href="">WOMEN</a>
              <a href="">MEN</a>
              <a href="">KIDS</a>
              <a href="/shop">SHOP</a>
            </div>
            <div class="menu-bottom">
              <a class="frag" href="Fragrance.html">FRAGRANCE</a>
              <div class="inner-bottom">
                <a href="#">T-SHIRT AND POLO</a>
                <div class="inner-inner">
                  <a href="#">T-SHIRT</a>
                  <a href="#">POLO</a>
                </div>
              </div>
              <div class="inner-bottom">
                <a href="#">PANTS</a>
                <div class="inner-inner">
                  <a href="#">CHINOS</a>
                  <a href="#">JEANS</a>
                </div>
              </div>
              <div class="inner-bottom">
                <a href="">SHIRTS
                  <div class="inner-inner">
                    <a href="#">CASUAL SHIRT</a>
                    <a href="#">FORMAL SHIRTS</a>
                  </div>
                </a>
              </div>
              <div class="inner-bottom">
                <a href="">ACCESSORIES
                  <div class="inner-inner">
                    <a href="#">MAN WALLETS</a>
                    <a href="#">BELTS</a>
                    <a href="#">CARD HOLDER</a>
                  </div>
                </a>
              </div>
              <div class="inner-bottom">
                <a href="">UNSTITCHED
                  <div class="inner-inner">
                    <a href="#">COTTON</a>
                    <a href="#">WASH & WEAR</a>
                  </div>
                </a>
              </div>
              <div class="inner-bottom">
                <a href="">EASTERN WEAR
                  <div class="inner-inner">
                    <a href="#">SHALWAR KAMEEZ</a>
                    <a href="#">KURTA</a>
                  </div>
                </a>
              </div>
              <div class="inner-bottom">
                <a href="">OUTWEAR
                  <div class="inner-inner">
                    <a href="#">HOODIES</a>
                    <a href="#">SWEAT SHIRTS</a>
                    <a href="#">JACKETS</a>
                  </div>
                </a>
              </div>
              <a class="home" href="">HOME</a>
              <a href="" class="scan">#SCAN BARCODE</a>
            </div>
          </div>
        </div>
        <a class="logo" href="Index.html">OUTFITTERS</a>
      </div>
      <div class="right">
        <!-- Search -->
        <div class="item">
          <a href="#">
            <img class="icon" src="https://img.icons8.com/ios-filled/25/ffffff/search.png" alt="Search Icon">
          </a>
          <a class="search">SEARCH</a>
        </div>

        <!-- Admin -->
        <div class="item">
          <% if (isAdmin) { %>
            <a href="/admin/dashboard">
              <img class="icon" src="https://img.icons8.com/ios-filled/25/ffffff/admin-settings-male.png" alt="Admin Icon">
            </a>
            <a href="/admin/dashboard" class="search">Admin</a>
          <% } else { %>
            <a href="/admin/login">
              <img class="icon" src="https://img.icons8.com/ios-filled/25/ffffff/admin-settings-male.png" alt="Admin Icon">
            </a>
            <a href="/admin/login" class="search">Admin</a>
          <% } %>
        </div>

        <!-- Cart -->
        <div class="item">
          <a href="#">
            <img class="icon" src="https://img.icons8.com/ios-filled/25/ffffff/shopping-cart.png" alt="Cart Icon">
          </a>
          <a class="cart">CART</a>
        </div>
        <!-- Login/Logout -->
        <div class="item">
          <% if (!isAuthenticated) { %>
            <a href="/login">
              <img class="icon" src="https://img.icons8.com/ios-filled/25/ffffff/login-rounded-right.png"
                alt="Login Icon">
            </a>
            <a href="/login" class="login">LOGIN</a>
            <% } else { %>
              <a href="/logout">
                <img class="icon" src="https://img.icons8.com/ios-filled/25/ffffff/logout-rounded-left.png"
                  alt="Logout Icon">
              </a>
              <a href="/logout" class="login">LOGOUT</a>
              <% } %>
        </div>
      </div>

      <style>
        /* Main Right Container */
        .right {
          display: flex;
          align-items: center;
          /* Align items vertically */
          justify-content: flex-end;
          /* Push items to the right */
          gap: 20px;
          /* Adjust spacing between items */
          margin-top: -40px;
          /* Move the entire container higher */
          position: relative;
        }

        /* Individual Item */
        .item {
          display: flex;
          flex-direction: column;
          align-items: center;
          text-align: center;
        }

        /* Icon Styling */
        .item .icon {
          width: 25px;
          height: 25px;
          margin-bottom: 5px;
          /* Adjust spacing between icon and text */
          cursor: pointer;
        }

        /* Link/Text Styling */
        .item a {
          font-size: 12px;
          color: white;
          font-weight: 500;
          text-decoration: none;
        }

        /* Hover Effects */
        .item a:hover {
          color: #e0e0e0;
        }

        .item img:hover {
          filter: brightness(1.2);
        }
      </style>

    </div>
    <!-- Carousel for Background Images -->
    <div id="backgroundCarousel" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="images/men1.webp" class="d-block w-100" alt="T-Shirt and Polo">
        </div>
        <div class="carousel-item">
          <video class="d-block w-100" width="100%" height="100%" autoplay muted loop playsinline>
            <source src="images/kidsvid.mp4" type="video/mp4">
            Your browser does not support the video tag.
          </video>

        </div>
        <div class="carousel-item">
          <img src="images/women1.webp" class="d-block w-100" alt="Women Wear">
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#backgroundCarousel" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#backgroundCarousel" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>

    <div class="middle">
      <div class="mleft">
        <a href="#backgroundCarousel" class="d-flex align-items-center text-white prev-btn" data-bs-slide="prev">
          <img class="arrow" src="images/left-arrow.png" alt="Previous">

        </a>
      </div>
      <div class="mright">
        <a href="#backgroundCarousel" class="d-flex align-items-center text-white next-btn" data-bs-slide="next">

          <img class="arrow" src="images/rightarrow.png" alt="Next">
        </a>
      </div>
    </div>
    <div class="bottom">
      <a class="bottom-menu">WINTER EDIT24/25</a>
      <a class="bottom-menu">ESSENTIALS</a>
      <a class="bottom-menu">SPECIAL PRICES</a>
    </div>
    <!-- For 750px and below -->
    <div class="bottom-smaller">
      <a href="#">
        <div class="text">SHIRT</div>
        <div class="explore">SEARCH AND EXPLORE</div>
      </a>
    </div>


    <!-- Slide 1 -->
